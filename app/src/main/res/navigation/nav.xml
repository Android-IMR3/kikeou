<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:startDestination="@id/listFragment">
    <fragment
        android:id="@+id/listFragment"
        android:name="fr.enssat.kikeou.alnezami_dansay.fragment.ListFragment"
        android:label="fragment_list"
        tools:layout="@layout/fragment_list" >
        <action
            android:id="@+id/action_listFragment_to_detaillsFragment"
            app:destination="@id/detaillsFragment" />
        <action
            android:id="@+id/action_listFragment_to_qrScannerFragment"
            app:destination="@id/qrScannerFragment" />
        <action
            android:id="@+id/action_listFragment_to_QRFragment"
            app:destination="@id/QRFragment" />
        <action
            android:id="@+id/action_listFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_listFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/addDataFragment"
        android:name="fr.enssat.kikeou.alnezami_dansay.fragment.AddDataFragment"
        android:label="fragment_add_data"
        tools:layout="@layout/fragment_add_data" />
    <fragment
        android:id="@+id/detaillsFragment"
        android:name="fr.enssat.kikeou.alnezami_dansay.fragment.detaillsFragment"
        android:label="fragment_detaills"
        tools:layout="@layout/fragment_detaills" >
        <action
            android:id="@+id/action_detaillsFragment_to_agendaFragment"
            app:destination="@id/agendaFragment" />
        <action
            android:id="@+id/action_detaillsFragment_to_listFragment"
            app:destination="@id/listFragment" />
    </fragment>
    <fragment
        android:id="@+id/agendaFragment"
        android:name="fr.enssat.kikeou.alnezami_dansay.fragment.AgendaFragment"
        android:label="fragment_agenda"
        tools:layout="@layout/fragment_agenda" />
    <fragment
        android:id="@+id/QRFragment"
        android:name="fr.enssat.kikeou.alnezami_dansay.fragment.QRFragment"
        android:label="fragment_q_r"
        tools:layout="@layout/fragment_q_r" >
        <action
            android:id="@+id/action_QRFragment_to_addDataFragment"
            app:destination="@id/addDataFragment" />
    </fragment>
    <fragment
        android:id="@+id/qrScannerFragment"
        android:name="fr.enssat.kikeou.alnezami_dansay.fragment.QrScannerFragment"
        android:label="fragment_qr_scanner"
        tools:layout="@layout/fragment_qr_scanner" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="fr.enssat.kikeou.alnezami_dansay.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_homeUpdateFragment"
            app:destination="@id/homeUpdateFragment" />
        <action
            android:id="@+id/action_homeFragment_to_listFragment"
            app:destination="@id/listFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeUpdateFragment"
        android:name="fr.enssat.kikeou.alnezami_dansay.HomeUpdateFragment"
        android:label="fragment_home_update"
        tools:layout="@layout/fragment_home_update" >
        <action
            android:id="@+id/action_homeUpdateFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="fr.enssat.kikeou.alnezami_dansay.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" />
</navigation>